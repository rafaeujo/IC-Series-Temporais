XI <- read.table("Data_CPDO.txt")
#NO
ts.plot(XI$NO)
x11()
par(mfrow=c(2,1))
acf(XI$NO)
pacf(XI$NO)
fitNO <- arima(XI$NO,order=c(1,0,1))
x11()
par(mfrow=c(2,1))
acf(fitNO$residuals)
pacf(fitNO$residuals)
